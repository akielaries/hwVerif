#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55ae50af7040 .scope module, "block_bench" "block_bench" 2 1;
 .timescale 0 0;
v0x55ae50b094b0_0 .var "clock", 0 0;
v0x55ae50b09580_0 .var "in_a", 0 0;
v0x55ae50b09650_0 .net "res", 0 0, v0x55ae50b09390_0;  1 drivers
S_0x55ae50af71d0 .scope module, "uut" "block" 2 6, 3 1 0, S_0x55ae50af7040;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in_a";
    .port_info 1 /INPUT 1 "clock";
    .port_info 2 /OUTPUT 1 "res";
v0x55ae50abf7f0_0 .net "clock", 0 0, v0x55ae50b094b0_0;  1 drivers
v0x55ae50abfc00_0 .net "in_a", 0 0, v0x55ae50b09580_0;  1 drivers
v0x55ae50b09390_0 .var "res", 0 0;
E_0x55ae50abe420 .event posedge, v0x55ae50abf7f0_0;
    .scope S_0x55ae50af71d0;
T_0 ;
    %wait E_0x55ae50abe420;
    %load/vec4 v0x55ae50abfc00_0;
    %assign/vec4 v0x55ae50b09390_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55ae50af7040;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ae50b094b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ae50b09580_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55ae50af7040;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x55ae50b094b0_0;
    %inv;
    %store/vec4 v0x55ae50b094b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55ae50af7040;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x55ae50b09580_0;
    %inv;
    %store/vec4 v0x55ae50b09580_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55ae50af7040;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "block_bench.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55ae50af7040 {0 0 0};
    %vpi_call 2 18 "$monitor", $time, " clock = %1b res = %4b", v0x55ae50b094b0_0, v0x55ae50b09580_0, v0x55ae50b09650_0 {0 0 0};
    %delay 99, 0;
    %vpi_call 2 19 "$display", "wrapping up" {0 0 0};
    %delay 1, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "block_bench.v";
    "block.v";
